{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:react/recommended",
    "plugin:prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "jsx-quotes": [2, "prefer-single"],
    "semi": ["error", "never"],
    "quotes": [2, "single"],
    "indent": ["error", 2]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
